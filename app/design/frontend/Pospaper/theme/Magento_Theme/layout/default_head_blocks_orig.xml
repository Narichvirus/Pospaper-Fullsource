<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1.0"/>
        <css src="mage/calendar.css"/>    
        <css src="css/styles.css"/>   
        <css src="css/my-account.css"/>  
        <css src="css/mediaquery.css"/> 
        <css src="css/animate.css"/> 
        <css src="css/jquery.mmenu.all.css"/>  
        <css src="css/jquery.mCustomScrollbar.css"/>
        <css src="css/easy-responsive-tabs.css"/>
        <css src="css/price-match.css"/>
        <css src="css/owl.carousel.min.css"/>                        
        <script src="js/jquery-latest.js"/>
